<!--네비게이션-->
{{>layouts/header}}



{{#posts}}
    <div class="container">
        <div class="mb-3 mt-4">
        <label class="form-label">제목</label>
        <input type="text" class="form-control" name="title" readonly
               value={{title}}>
    </div>

    <div class="mb-3">
        <label class="form-label">내용</label>
        <input style="line-height: 50em;" type="text" class="form-control"  name="content" readonly
               value={{content}}>

    </div>
{{/posts}}

<div class="d-grid gap-2 d-md-flex justify-content-md-end">
    <a href="/articles/noteUpdate/{{id}}" class="btn btn-dark ">수정</a>
    <a href="/articles/noteDelete/{{id}}" class="btn btn-dark ">삭제</a>
</div>
</div>



{{>layouts/footer}}